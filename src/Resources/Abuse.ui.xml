<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.buttonDiv {
		float: right;
		font-weight: bold;
		}
	</ui:style>
<g:DialogBox text="Please report abuse">
<g:HTMLPanel>

<g:TextArea></g:TextArea>
	
		
	<g:HTMLPanel>
	    
		<g:Button styleName="{style.buttonDiv}" ui:field="button" />
	
	</g:HTMLPanel>

</g:HTMLPanel>
</g:DialogBox>
</ui:UiBinder> 